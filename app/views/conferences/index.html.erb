<p id="notice"><%= notice %></p>

<h1>Conferências</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conferences.each do |conference| %>
      <tr>
        <td><%= link_to conference.name, conference %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Criar uma nova Conferência', new_conference_path %> ou
<%= link_to 'Criar a partir de um arquivo', import_new_lectures_path %>
